<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <configSections>
        <section name="JXTPortal.Data" type="JXTPortal.Data.Bases.NetTiersServiceSection, JXTPortal.Data" allowDefinition="MachineToApplication" restartOnExternalChanges="true" />
    </configSections>
    <appSettings>
        <add key="AdvertiserExcelLocation" value="C:\Users\daniel.ng\Desktop\Lifestyle Careers - data export\lifestyl_jobboard_advertisers.xls"/>
        <add key="AdvertiserUserExcelLocation" value="C:\Users\daniel.ng\Desktop\Master List - jobx - country - location area.xlsx"/>
        <add key="LogLocation" value="C:\Users\daniel.ng\Desktop\minilog.txt"/>
    </appSettings>
    <connectionStrings>
        <add name="JXTPortal.Data.ConnectionString" connectionString="Data Source=JXT-WS01;Initial Catalog=MiniJXT;Persist Security Info=True;User ID=sa;Password=nameofthegame" />
    </connectionStrings>
    <JXTPortal.Data defaultProvider="SqlNetTiersProvider">
        <providers>
            <!--
				*** SqlClient Provider ***
					connectionStringName: sqlclient connection string to the db
					useStoredProcedure: if true, indicates that we use the stored procedures, otherwise, we use parametrized queries that are embedded.
			-->
            <add name="SqlNetTiersProvider" type="JXTPortal.Data.SqlClient.SqlNetTiersProvider, JXTPortal.Data.SqlClient" connectionStringName="JXTPortal.Data.ConnectionString" providerInvariantName="System.Data.SqlClient" entityFactoryType="JXTPortal.Entities.EntityFactory" useEntityFactory="true" enableEntityTracking="false" enableMethodAuthorization="false" useStoredProcedure="true" defaultCommandTimeout="30" />
            <!-- 
				*** WebserviceClient Provider ***
				The url parameter indicates the webservices url (ex: http://localhost/JXTPortal/JXTPortalServices.aspx)
				<add 
					name="WsNetTiersProvider" 
					type="JXTPortal.Data.WebServiceClient.WsNetTiersProvider, JXTPortal.Data.WebServiceClient"
					url="/JXTPortalServices.asmx" />
			-->
        </providers>
    </JXTPortal.Data>
    <startup>
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
    </startup>
</configuration>
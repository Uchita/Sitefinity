@using System.Web.Mvc;
@using Telerik.Sitefinity.Frontend.Mvc.Helpers;
@using Telerik.Sitefinity.Modules.Pages;
@using Telerik.Sitefinity.UI.MVC;
@using Telerik.Sitefinity.Services;

<!DOCTYPE html>
<html @Html.RenderLangAttribute()>
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <meta charset="utf-8" />
    <title></title>
    <!-- Global site tag (gtag.js) - Google Analytics -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=UA-2176719-15"></script>
    <script>
        window.dataLayer = window.dataLayer || [];
        function gtag() { dataLayer.push(arguments); }
        gtag('js', new Date());
        gtag('config', 'UA-74680852-1');
    </script>
    <!-- Favicon -->
    <meta name="mobile-web-app-capable" content="yes">
    <link rel="image_src" href="/ResourcePackages/EnterpriseRecruitment/assets/dist/images/logo-og.png" />
    <link rel="shortcut icon" href="/ResourcePackages/EnterpriseRecruitment/assets/dist/images/favicon.ico" />
    <link rel="mask-icon" href="/ResourcePackages/EnterpriseRecruitment/assets/dist/images/website_icon.svg" color="#00a0a0">
    <link rel="icon" sizes="192x192" href="/ResourcePackages/EnterpriseRecruitment/assets/dist/images/nice-highres.png" />
    <link rel="icon" sizes="128x128" href="/ResourcePackages/EnterpriseRecruitment/assets/dist/images/niceicon.png" />
    <link rel="apple-touch-icon" sizes="128x128" href="/ResourcePackages/EnterpriseRecruitment/assets/dist/images/niceicon.png" />
    <link rel="apple-touch-icon" sizes="114x114" href="/ResourcePackages/EnterpriseRecruitment/assets/dist/images/apple-icon-144x144.png">
    <link rel="apple-touch-icon" sizes="152x152" href="/ResourcePackages/EnterpriseRecruitment/assets/dist/images/touch-icon-ipad.png">
    <link rel="apple-touch-icon" sizes="167x167" href="/ResourcePackages/EnterpriseRecruitment/assets/dist/images/touch-icon-ipad-retina.png">
    <link rel="apple-touch-icon" sizes="180x180" href="/ResourcePackages/EnterpriseRecruitment/assets/dist/images/touch-icon-iphone-retina.png">
    <meta name="msapplication-TileColor" content="#00a0a0">
    <meta name="theme-color" content="#ffffff">
    <link rel="stylesheet" href="/ResourcePackages/EnterpriseRecruitment/manifest.json">
    <link href='https://www.enterprise.co.nz/http_fontsgoogleapiscom/css?family=Open+Sans:400,300,300italic,400italic,600,700,600italic,700italic,800,800italic%7CRoboto:400,100,100italic,300,300italic,400italic,500,500italic,700,700italic,900,900italic%7CTitillium+Web:400,300,600,700italic,700,600italic,400italic,300italic' rel='stylesheet' type='text/css'>


    @Html.Section("head")
    @Html.StyleSheet(Url.Content("~/ResourcePackages/EnterpriseRecruitment/assets/dist/css/fontawesome.css"), "head")
    @Html.StyleSheet(Url.Content("~/ResourcePackages/EnterpriseRecruitment/assets/dist/css/main.css"), "head")

    @Html.Partial("~/ResourcePackages/EnterpriseRecruitment/MVC/partial/TagManagerHead.cshtml")
</head>

<body>
    @Html.Partial("~/ResourcePackages/EnterpriseRecruitment/MVC/partial/TagManagerBody.cshtml")
    @Html.Section("top")
    <div id="wrapper" class="page-wrapper clearfix">
        <div id="header">
            <a href="main-content" id="skip-link">Skip to content</a>
            <div class="loader"></div>
            @Html.SfPlaceHolder("Header")
        </div>
        <main id="main-content">
            @Html.SfPlaceHolder("Main")
        </main>
        <div id="footer">
            <footer id="footer-container">
                @Html.SfPlaceHolder("Footer")
            </footer>
        </div>
    </div>
    @* Inline editing scripts and helpers. They should be available only when inline editing is possible. *@
    @if (Html.ShouldRenderInlineEditing())
    {
        @Html.Script(ScriptRef.MicrosoftAjax, "top")
        @Html.Script(ScriptRef.MicrosoftAjaxCore, "top")
        @Html.Script(ScriptRef.JQuery, "top")
    }

    @if (!Html.ShouldRenderInlineEditing() && !Telerik.Sitefinity.Services.SystemManager.IsDesignMode)
    {
        @Html.Script(ScriptRef.JQuery, "top")
    }

    @if (Html.ShouldRenderInlineEditing())
    {
        @Html.Script(Url.EmbeddedResource("Telerik.Sitefinity.Resources.Reference", "Telerik.Sitefinity.Resources.Scripts.jquery.ba-outside-events.min.js"), "top")
        @Html.Script(ScriptRef.KendoAll, "top")
        @Html.Script(ScriptRef.KendoTimezones, "top")
        @Html.Script(Url.EmbeddedResource("Telerik.Sitefinity.Resources.Reference", "Telerik.Sitefinity.Resources.Scripts.RequireJS.require.min.js"), "top")
        @Html.InlineEditingManager(false)
    }
    <script>
        window.onerror = function () {
            $("#loading-screen, .loader, #jxt-loading-screen").hide();
        }
    </script>
    @Html.Section("bottom")
    @Html.Script("/ResourcePackages/EnterpriseRecruitment/assets/dist/js/bootstrap.min.js", "bottom")
    @Html.Script("/ResourcePackages/EnterpriseRecruitment/assets/dist/js/responsiveCarousel.js", "bottom")
    @Html.Script("/ResourcePackages/EnterpriseRecruitment/assets/dist/js/modernizr.custom.21882.js", "bottom")
    @Html.Script("/ResourcePackages/EnterpriseRecruitment/assets/dist/js/jquery.simplyscroll.min.js", "bottom")
    @Html.Script("/ResourcePackages/EnterpriseRecruitment/assets/dist/js/responsive-plugins-0.js", "bottom")
    @Html.Script("/ResourcePackages/EnterpriseRecruitment/assets/dist/js/owl.carousel.js", "bottom")
    @Html.Script("/ResourcePackages/EnterpriseRecruitment/assets/dist/js/isotope.pkgd.min.js", "bottom")
    @Html.Script("/ResourcePackages/EnterpriseRecruitment/assets/dist/js/imagesloaded_isotope.js", "bottom")
    @Html.Script("/ResourcePackages/EnterpriseRecruitment/assets/dist/js/project.min.js", "bottom")
    @Html.Script("/ResourcePackages/EnterpriseRecruitment/assets/dist/js/modernizr.custom.js", "bottom")
    @Html.Script("/ResourcePackages/EnterpriseRecruitment/assets/dist/js/jquery.dlmenu.js", "bottom")

    <!-- Slider plugin -->
    @*<script src="/media/enterprise/js/strapslide/jquery.touchSwipe.min.js"></script>
    <script src="/media/enterprise/js/strapslide/jquery.transit.min.js"></script>
    <script src="/media/enterprise/js/strapslide/jquery.mousewheel.js"></script>
    <script src="/media/enterprise/js/strapslide/jquery.fitVids.js"></script>
    <script src="/media/enterprise/js/strapslide/strapslide.js"></script>*@


</body>
</html>
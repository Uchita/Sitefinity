<?xml version="1.0"?>
<configuration>
  <configSections>
    <section name="JXTPortal.Data" type="JXTPortal.Data.Bases.NetTiersServiceSection, JXTPortal.Data" allowDefinition="MachineToApplication" restartOnExternalChanges="true"/>
    <sectionGroup name="userSettings" type="System.Configuration.UserSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" >
      <section name="JXTPostJobApplicationToFTP.Settings1" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false" />
    </sectionGroup>
  </configSections>
  <appSettings>

    <add key="AdminEmail" value="naveen@jxt.com.au"/>
    <add key="SiteMemberXML" value="C:\Developments\jxtplatform\JXTPostDataToFTP\bin\Debug\SiteMemberConfiguration.xml"/>
    <add key="MemberXMLExportFolder" value="C:\Developments\jxtplatform\JXTPostDataToFTP\bin\Debug\" />
    
    <!-- XML Files Paths -->
    <add key="XMLFilesPath" value="C:\Developments\jxtplatform\JXTPortal.Website\xml\" />
    
    <add key="MemberUploadPicturePaths" value="/media/candidate/" />

  </appSettings>
  <system.net>
    <mailSettings>
      <smtp deliveryMethod="SpecifiedPickupDirectory">
        <network host="ignored" />
        <specifiedPickupDirectory pickupDirectoryLocation="C:\JobXMail" />
      </smtp>
      <!--<smtp deliveryMethod="Network" from="testuser@jxt.com">
        <network defaultCredentials="false" host="jobx-svr01.jobx.local" port="25" userName="Daniel.Ng" password="d@1234"/>
      </smtp>-->
    </mailSettings>
  </system.net>
  <connectionStrings>
    <add name="JXTPortal.Data.ConnectionString" connectionString="Data Source=JXT-WS01;Initial Catalog=MiniJXT;Persist Security Info=True;User ID=sa;Password=nameofthegame"/>
  </connectionStrings>
  <JXTPortal.Data defaultProvider="SqlNetTiersProvider">
    <providers>
      <!--
                           *** SqlClient Provider ***
                                  connectionStringName: sqlclient connection string to the db
                                  useStoredProcedure: if true, indicates that we use the stored procedures, otherwise, we use parametrized queries that are embedded.
                     -->
      <add name="SqlNetTiersProvider" type="JXTPortal.Data.SqlClient.SqlNetTiersProvider, JXTPortal.Data.SqlClient" connectionStringName="JXTPortal.Data.ConnectionString" providerInvariantName="System.Data.SqlClient" entityFactoryType="JXTPortal.Entities.EntityFactory" useEntityFactory="true" enableEntityTracking="false" enableMethodAuthorization="false" useStoredProcedure="true" defaultCommandTimeout="30"/>
      <!--
                           *** WebserviceClient Provider ***
                           The url parameter indicates the webservices url (ex: http://localhost/JXTPortal/JXTPortalServices.aspx)
                           <add
                                  name="WsNetTiersProvider"
                                  type="JXTPortal.Data.WebServiceClient.WsNetTiersProvider, JXTPortal.Data.WebServiceClient"
                                  url="/JXTPortalServices.asmx" />
                     -->
    </providers>
  </JXTPortal.Data>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
  </startup>
  <system.diagnostics>
    <trace autoflush="true" indentsize="4">
      <listeners>
        <add name="myListener" type="System.Diagnostics.TextWriterTraceListener" initializeData="JXTPostJobApplicationToFTP.log" />
        <remove name="Default" />
      </listeners>
    </trace>
  </system.diagnostics>
</configuration>

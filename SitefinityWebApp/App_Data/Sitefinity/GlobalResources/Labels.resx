  <data name="RegistrationSuccessEmail">
    <value>&lt;p&gt;&lt;img src="https://&lt;%\s*SiteName\s*%&gt;/ResourcePackages/Bayside/assets/dist/images/bayside_logo_white.svg" alt="logo" /&gt;&lt;/p&gt;

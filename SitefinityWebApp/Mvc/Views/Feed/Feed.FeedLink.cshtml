@model Telerik.Sitefinity.Frontend.Publishing.Mvc.Models.FeedViewModel
@using Telerik.Sitefinity.Frontend.Publishing.Mvc.Models;

@* Feed Widget - eg. RSS *@
@if ((Model.InsertionOption == FeedInsertionOption.PageOnly || Model.InsertionOption == FeedInsertionOption.PageAndAddressBar) && !string.IsNullOrEmpty(Model.Url))
{
    <div class="o-rss-feed @Model.CssClass">
        <a href="@Model.Url" title="@Model.Tooltip" @(Model.OpenInNewWindow ? "target=\"_blank\"" : "")>
            <svg aria-hidden="true">
                <use xlink:href="~/ResourcePackages/JXTNextMRP/assets/dist/sprites/solid.svg#rss-square"></use>
            </svg>
            @Model.Title
        </a>
    </div>
}
    